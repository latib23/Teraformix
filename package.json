  "scripts": {
    "dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vite build",
    "build:server": "nest build",
    "build:ts": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "typeorm migration:generate src/migrations/AutoMigration -d src/database/data-source.ts",
    "migration:run": "typeorm migration:run -d dist/database/data-source.js",
    "migration:revert": "typeorm migration:revert -d dist/database/data-source.js"
  },
